      <node>
        <text-node>
          <description>Removes customer cookies set by Ordergroove on customer sign out.</description>
        </text-node>
        <node-display width="3" x="5" y="1"/>
      </node>
    </segment>
  </branch>
  <branch basename="RemoveCustomerCookies">
