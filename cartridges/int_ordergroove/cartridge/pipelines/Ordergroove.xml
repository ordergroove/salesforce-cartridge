          <description>Retrieves JSON settings for Ordergroove offers to populate on the cart page.</description>
        </text-node>
        <node-display width="3" x="1" y="7"/>
      </node>
    </segment>
  </branch>
  <branch basename="_ANONYMOUS_BRANCH_10">
    <segment>
      <node>
        <text-node>
          <description>Checks if a subscription item is in the basket based on an Ordergroove cookie or via basket.</description>
        </text-node>
        <node-display width="3" x="5" y="7"/>
      </node>
    </segment>
  </branch>
  <branch basename="_ANONYMOUS_BRANCH_11">
    <segment>
      <node>
        <text-node>
          <description>Checks to see if customer has an item in the basket on subscription and is authenticated.  If the condition is not satisfied, a redirect will take place.  If the condition is satisfied, this will populate Ordergroove settings for checkout.</description>
        </text-node>
        <node-display width="3" x="9" y="7"/>
      </node>
    </segment>
  </branch>
  <branch basename="_ANONYMOUS_BRANCH_12">
    <segment>
      <node>
        <text-node>
          <description>Replaces the applicable payment methods with a filtered list based on subscription being in the cart.</description>
        </text-node>
        <node-display width="3" x="13" y="7"/>
      </node>
    </segment>
  </branch>
  <branch basename="CartSettings">
    <segment>
      <node>
        <start-node call-mode="private" name="CartSettings" secure="false"/>
        <node-display x="1" y="8"/>
      </node>
      <simple-transition>
        <transition-display>
          <bend-point relative-to="source" x="0" y="1"/>
